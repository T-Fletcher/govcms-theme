name = govcms.gov.au Bootstrap theme
description = A lightweight responsive theme based on Bootstrap.
core = 7.x

; based off static HTML mockup version:
; https://fribibb.github.io/new-govcms/

settings[breadcrumb_display]= 1
settings[scrolltop_display]= 1
settings[responsive_respond]= 1
settings[bootstrap_css_cdn]= 3.3.5
; number of columns in each megamenu drop down
settings[sub_menu_columns] = 4;
; number of items in homepage carousel
settings[num_carousel_items] = 8;

scripts[] = js/bootstrap.min.js
scripts[] = js/jquery.js
scripts[] = js/govcms.js

stylesheets[all][] = css/styles.css

regions[header] = Header
regions[content] = Content
regions[sidebar_first] = Sidebar First
regions[sidebar_second] = Sidebar Second
regions[footer] = Footer
regions[page_top] = Page top
regions[page_bottom] = Page bottom

